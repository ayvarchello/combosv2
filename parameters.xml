<simulation>
    <simulation_time>10000</simulation_time>			 	<!-- Simulation time in hours -->

    <!-- Server side -->
    <server_side>
        <n_projects>1</n_projects>				<!-- Number of projects -->
        <sproject>
            <snumber>0</snumber>				<!-- Project number -->
            <name>PROJECT1</name>				<!-- Project name -->
            <long_precentage>0.0</long_precentage>    <!-- Computational power share of long applications -->
            <nscheduling_servers>1</nscheduling_servers>	<!-- Number of scheduling servers -->
            <ndata_servers>1</ndata_servers>		<!-- Number of data servers of the project -->
            <server_pw>12000000000.0</server_pw>		<!-- Server power in FLOPS -->
            <disk_bw>80000000</disk_bw>			<!-- Disk speed in bytes/sec -->
            <ifgl_percentage>0</ifgl_percentage>		<!-- Percentage of input files generated locally -->
            <ifcd_percentage>100</ifcd_percentage>		<!-- Percentage of times clients must download new input files (they can't use old input files) -->
            <nplatforms>5</nplatforms>
            <napplications>6</napplications>
            <application>
                <name>APP1</name>
                <input_file_size>31457280</input_file_size>	<!-- Input file size in bytes -->
                <task_fpops>3000000000000</task_fpops>		<!-- Task duration in flops -->
                <output_file_size>10485760</output_file_size>	<!-- Answer size in bytes -->
                <min_quorum>2</min_quorum>			<!-- Number of times a workunit must be received in order to validate it-->
                <target_nresults>3</target_nresults>		<!-- Number of results to create initially -->
                <max_error_results>4</max_error_results>	<!-- Max number of erroneous results -->
                <max_total_results>8</max_total_results>	<!-- Max number of total results -->
                <max_success_results>8</max_success_results>	<!-- Max number of success results -->
                <delay_bound>172800</delay_bound>		<!-- Task deadline -->	
                <success_percentage>95</success_percentage>	<!-- Percentage of success results -->
                <canonical_percentage>95</canonical_percentage> <!-- Percentage of success results that make up a consensus -->
                <replication>1</replication>			<!-- Input files replication -->
                <ntotal_tasks>70000</ntotal_tasks>     <!-- Number of tasks in the application -->
                <start_time>0</start_time> <!-- Application start time in seconds -->
                <platform>0</platform>
                <platform>1</platform>
                <platform>2</platform>
                <platform>3</platform>
                <platform>4</platform>
            </application>
